# Configurações para sincronização de banco de dados
# Copie para .env e ajuste os valores

# Banco Local
MYSQL_HOST=localhost
MYSQL_USER=root
MYSQL_PASSWORD=sua_senha_local
MYSQL_DATABASE=admin_system

# VPS/Servidor Remoto
VPS_HOST=seu-vps.com
VPS_SSH_USER=root
VPS_PROJECT_PATH=/var/www/seu-projeto

# Banco no VPS
VPS_MYSQL_HOST=localhost
VPS_MYSQL_USER=root
VPS_MYSQL_PASSWORD=sua_senha_vps
VPS_MYSQL_DATABASE=admin_system